<div>
    <ul>
      <% if current_user %>
        <h3><%= current_user.email %></h3>
      <% else %>
        <h3>please <%= link_to "Log In", login_path %></h3>
      <% end %>
      <p><%= link_to "Account", account_path %> </p>
